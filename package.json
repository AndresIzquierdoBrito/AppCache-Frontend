    "@mantinex/dev-icons": "^1.0.2",
    "@tanstack/react-query": "^5.29.2",
    "i18next": "^23.11.2",
    "i18next-browser-languagedetector": "^7.2.1",
    "i18next-http-backend": "^2.5.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^14.1.0",
    "react-router-dom": "^6.22.3"
