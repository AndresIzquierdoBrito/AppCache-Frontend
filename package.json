    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "postcss": "^8.4.38",
    "postcss-preset-mantine": "^1.14.4",
